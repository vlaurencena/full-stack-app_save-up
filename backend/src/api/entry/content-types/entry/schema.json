{
  "kind": "collectionType",
  "collectionName": "entries",
  "info": {
    "singularName": "entry",
    "pluralName": "entries",
    "displayName": "Entry"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "string",
      "required": true
    },
    "isIncome": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "entries"
    }
  }
}
